<?xml version='1.0' encoding='utf-8' ?>
<!--
	resourcespath 指明一个 Resources 目录，srcroot 指定根目录，extname 指定哪些扩展名的需要打包成 unity3d， ignoreext 指定哪些文件不需要处理，剩下的文件直接拷贝过去
-->
<Root>
	<resourcespath srcroot="Assets/Prefabs/Resources" unity3dextname="prefab" ignoreext="meta" />
	<resourcespath srcroot="Assets/Scenes" unity3dextname="unity" ignoreext="meta,xml,exr" />
</Root>