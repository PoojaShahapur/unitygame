<?xml version='1.0' encoding='utf-8' ?>
<!--
	resourcespath 指明一个 Resources 目录，srcroot 指定根目录，extname 指定哪些扩展名的需要打包成 unity3d， ignoreext 指定哪些文件不需要处理，剩下的文件直接拷贝过去
	destroot 指定目标目录
	null 扩展名字是指没有扩展名字 ""
	reslistoutpath 导出的资源列表输出目录
-->
<Root reslistoutpath="E:/aaaaa/File2Unity3d.bytes">
	<resourcespath srcroot="Assets/Prefabs/Resources" destroot="" unity3dextname="prefab,png,shader,mat" ignoreext="meta" />
	<resourcespath srcroot="Assets/Scenes" destroot="Scenes" unity3dextname="unity" ignoreext="meta,xml,exr" />
</Root>