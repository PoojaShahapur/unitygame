<?xml version='1.0' encoding='utf-8' ?>
<!--  
	ResType 就是资源的类型，如 level
	Pack 就是一个输出资源包
	PackItem 就是包中的一项
	根目录都是 Assets 目录
-->
<Root>
	<PackType type="bundle" inpath="Prefabs/UI" outpath="UI" >
		<Pack name="UIScrollForm.unity3d">
			<PackItem path="UIScrollForm.prefab" restype="prefab" />
		</Pack>
	</PackType>
	<PackType type="bundle" inpath="Table" outpath="Table" >
		<Pack name="Table.unity3d">
			<PackItem path="Table1.tbl" restype="textasset" />
			<PackItem path="Table2.tbl" restype="textasset" />
		</Pack>
	</PackType>
	<PackType type="level" inpath="Scenes" outpath="Scene" >
		<Pack name="cave.unity3d">
			<PackItem path="cave.unity" />
		</Pack>
	</PackType>
</Root>