DELIMITER //
CREATE PROCEDURE INIT_LOGIN()
BEGIN
    DECLARE v_i int;
    SET v_i=10000001;
    WHILE v_i<=10000050 DO
	INSERT INTO LOGIN VALUES(v_i, 111111, 'zhanghao', 0, 0);
	SET v_i = v_i + 1;
    END WHILE;
END
//
DELIMITER ;
call INIT_LOGIN();

