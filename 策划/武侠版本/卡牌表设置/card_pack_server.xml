<?xml version="1.0" encoding="GB2312"?>
<!-- 打包配置文件 -->
<!-- solution 是一个方案 name 显示的方案的名字 cmd 打包完成后执行的配置文件 -->
<config>
	<table name="CardBase.tbl" ExcelFile="卡牌基本表.xlsx" tablename="CardBase">
		<fields>
			<field name="编号" type="int" size="4"/>
			<field name="名称" type="string" size="64" />
			<field name="类型" type="int" size="4"/>
			<field name="职业" type="int" size="4"/>
			<field name="种族" type="int" size="4"/>
			<field name="品质" type="int" size="4"/>
			<field name="魔法消耗" type="int" size="4"/>
			<field name="攻击力" type="int" size="4"/>
			<field name="血量" type="int" size="4"/>
			<field name="耐久" type="int" size="4"/>
			<field name="来源" type="int" size="4"/>
			<field name="嘲讽" type="int" size="4"/>
			<field name="冲锋" type="int" size="4"/>
			<field name="风怒" type="int" size="4"/>
			<field name="潜行" type="int" size="4"/>
			<field name="圣盾" type="int" size="4"/>
			<field name="魔免" type="int" size="4"/>
			<field name="法术伤害增加" type="int" size="4"/>
			<field name="过载" type="int" size="4"/>
			<field name="法术" type="int" size="4"/>
			<field name="法术需要目标" type="int" size="4"/>
			<field name="战吼" type="int" size="4"/>
			<field name="战吼需要目标" type="int" size="4"/>
		</fields>
	</table>
</config>