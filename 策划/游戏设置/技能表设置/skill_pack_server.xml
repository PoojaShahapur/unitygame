<?xml version="1.0" encoding="GB2312"?>
<!-- 打包配置文件 -->
<!-- solution 是一个方案 name 显示的方案的名字 cmd 打包完成后执行的配置文件 -->
<config>
	<table name="SkillBase.tbl" ExcelFile="卡牌技能表.xlsx" tablename="SkillBase">
		<fields>
			<field name="编号" type="int" size="4"/>
			<field name="名称" type="string" size="32" />
			<field name="效果" type="string" size="256" />
			<field name="条件类型" type="int" size="4"/>
			<field name="条件ID" type="int" size="4"/>
			<field name="条件成立效果" type="int" size="4"/>
			<field name="条件不成立效果" type="int" size="4"/>
		</fields>
	</table>
</config>